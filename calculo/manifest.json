    ],

    "file_handlers": [
        {
            "action": "/",
            "accept": {
                "text/plain": [".txt", ".math"],
                "application/json": [".json"]
            }
        }
    ],

    "share_target": {
        "action": "/",
        "method": "GET",
        "params": {
            "title": "title",
            "text": "text",
            "url": "url"
        }
    },

    "edge_side_panel": {
        "preferred_width": 400
    },

    "launch_handler": {
